<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="countryGroupServices" class="es.weso.rest.RegionServices">
		<property name="countryGroupManager">
			<ref bean="countryGroupManager" />
		</property>
		<property name="countryManager">
			<ref bean="countryManager" />
		</property>
	</bean>
	
	<bean id="rootServices" class="es.weso.rest.RootServices">
		<property name="countryGroupManager">
			<ref bean="countryGroupManager" />
		</property>
		<property name="indicatorManager">
			<ref bean="indicatorManager" />
		</property>
	</bean>

	<bean id="countryServices" class="es.weso.rest.CountryServices">
		<property name="countryManager">
			<ref bean="countryManager" />
		</property>
	</bean>

	<bean id="observationServices" class="es.weso.rest.ObservationServices">
		<property name="observationManager">
			<ref bean="observationManager" />
		</property>
	</bean>
	
	<bean id="indicatorServices" class="es.weso.rest.IndicatorServices">
		<property name="indicatorManager">
			<ref bean="indicatorManager" />
		</property>
	</bean>

	<bean id="observationsServices" class="es.weso.rest.ObservationsServices">
		<property name="observationManager">
			<ref bean="observationManager" />
		</property>
	</bean>
	
	<bean id="componentServices" class="es.weso.rest.ComponentServices">
		<property name="componentManager">
			<ref bean="componentManager" />
		</property>
	</bean>
	
	<bean id="datasetServices" class="es.weso.rest.DatasetServices">
		<property name="datasetManager">
			<ref bean="datasetManager" />
		</property>
	</bean>
	
	<bean id="weightSchemaServices" class="es.weso.rest.WeightSchemaServices">
		<property name="weightSchemaManager">
			<ref bean="weightSchemaManager" />
		</property>
	</bean>

	<bean id="namedRegion" class="es.weso.wirouter.country.NamedRegion">
		<property name="countryGroupManager">
			<ref bean="countryGroupManager" />
		</property>
	</bean>

	<bean id="countryGroupManager" class="es.weso.business.impl.CountryGroupManager">
		<property name="countryGroupDataManager">
			<ref bean="countryGroupDataManager" />
		</property>
	</bean>

	<bean id="countryManager" class="es.weso.business.impl.CountryManager">
		<property name="countryDataManager">
			<ref bean="countryDataManager" />
		</property>
		<property name="countryGroupDataManager">
			<ref bean="countryGroupDataManager" />
		</property>
	</bean>

	<bean id="observationManager" class="es.weso.business.impl.ObservationManager">
		<property name="observationDataManager">
			<ref bean="observationDataManager" />
		</property>
		<property name="indicatorManager">
			<ref bean="indicatorManager" />
		</property>
		<property name="countryManager">
			<ref bean="countryManager" />
		</property>
	</bean>

	<bean id="indicatorManager" class="es.weso.business.impl.IndicatorManager">
		<property name="indicatorDataManager">
			<ref bean="indicatorDataManager" />
		</property>
	</bean>
	
	<bean id="componentManager" class="es.weso.business.impl.ComponentManager">
		<property name="componentDataManager">
			<ref bean="componentDataManager" />
		</property>
	</bean>
	
	<bean id="datasetManager" class="es.weso.business.impl.DatasetManager">
		<property name="datasetDataManager">
			<ref bean="datasetDataManager" />
		</property>
	</bean>
	
	<bean id="weightSchemaManager" class="es.weso.business.impl.WeightSchemaManager">
		<property name="weightSchemaDataManager">
			<ref bean="weightSchemaDataManager" />
		</property>
	</bean>

	<bean id="countryDataManager" class="es.weso.data.impl.CountryDataManager" />
	<bean id="countryGroupDataManager" class="es.weso.data.impl.CountryGroupDataManager" />
	<bean id="observationDataManager" class="es.weso.data.impl.ObservationDataManager" />
	<bean id="indicatorDataManager" class="es.weso.data.impl.IndicatorDataManager" />
	<bean id="componentDataManager" class="es.weso.data.impl.ComponentDataManager" />
	<bean id="datasetDataManager" class="es.weso.data.impl.DatasetDataManager" />
	<bean id="weightSchemaDataManager" class="es.weso.data.impl.WeightSchemaDataManager" />
</beans>
